  <%= stylesheet_link_tag 'user', media: 'all', 'data-turbolinks-track': 'reload' %>
<div class="header">
  <h1 class="user-heading">List of users</h1>
</div>


<% if @users.empty? %>
  No users
<% else %>
  <% @users.each do |user| %>
    <a href="<%= user_path(user) %>"><%= render user %></a>
    <br>
  <% end %>
<% end %>
